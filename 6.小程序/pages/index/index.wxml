<!--index.wxml-->
<view class="containerPage">
  <view class="userinfo">
    <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 获取头昵称 </button>
    <block wx:else>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
  <view class="usermotto">
    <text class="user-motto">{{motto}}</text>
  </view>
<view>{{x}}</view>
<button class='weui-btn' type='primary' plain='true'>135513</button>

<view class='weui-cells_title'>
      <view class="weui-cells weui-cells_after-title">

      <navigator url="/pages/Activity/Activity" class="weui-cell weui-cell_access" hover-class="weui-cell_active">

        <view class="weui-cell__hd">

          <image src="/image/TabIcon/selectedMain.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>

        </view>

        <view class="weui-cell__bd">cell standard</view>

        <view class="weui-cell__ft weui-cell__ft_in-access">说明文字</view>

      </navigator>
      </view>
</view>
</view>
