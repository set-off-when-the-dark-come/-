.Main{
  height: 95vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;

}



.HeadDataAndAddress
{
  opacity: 0;
  height: 130rpx;
  display: flex;
  align-items: center;
}
.date{
  font-size: 100rpx;
  font-weight: lighter;
}
.address{
  height: 40rpx;
  padding: 0rpx;
  margin-top: 2vh;
  margin-left: 8rpx;
  font-size: 36rpx;
  font-weight: lighter;
  letter-spacing: 4rpx;
  color: #333333;
}
.addressImg{
  margin-top: 3vh;
  margin-left: 30rpx;
  width: 40rpx;
  height: 40rpx;
}

.MainText{
  height: 75vh;
  width: 400rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.first{
  transform: translateY(60rpx);
  opacity: 0;
  writing-mode: vertical-rl;
  display: block;
  margin-left:100rpx;
}
.second{
  transform: translateY(-60rpx);
  opacity: 0;
  writing-mode: vertical-rl;
  display: block;
  margin-left: -100rpx;
}
.Count{
  opacity: 0;
  display: block;
  font-size: 90rpx;
}
.line{
  opacity: 0;
  display: block;
  width: 1rpx;
  height: 110rpx;
  background-color: #666666;
}
.self1{
  transform: translateY(60rpx);
  margin-left: 180rpx;
}
.self2{
  transform: translateY(-60rpx);
  margin-left: -180rpx;
}



.JumpButton{
  opacity: 0;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 90rpx;
  width: 500rpx;
}
.JumpButton image{
  width: 40rpx;
  height: 40rpx;
}
.JumpButton text{
  font-size: 28rpx;
  color: #fff;
}
.JumpButton button{
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #05cebd;
  border-radius: 40rpx;
  width: 185rpx;
  height: 80rpx;
  box-shadow: 10rpx 5rpx 10rpx #666666
}